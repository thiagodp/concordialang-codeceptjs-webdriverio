{
  "name": "concordialang-codeceptjs-webdriverio",
  "version": "0.3.5",
  "description": "Concordia Language plug-in to generate test scripts for CodeceptJS with WebDriverIO",
  "license": "AGPL-3.0",
  "readme": "readme.md",
  "homepage": "https://github.com/thiagodp/concordialang-codeceptjs-webdriverio#readme",
  "author": {
    "name": "Thiago Delgado Pinto",
    "email": "thiago-dp@bol.com.br"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thiagodp/concordialang-codeceptjs-webdriverio.git"
  },
  "bugs": {
    "url": "https://github.com/thiagodp/concordialang-codeceptjs-webdriverio/issues"
  },
  "keywords": [
    "concordialang",
    "concordia-lang",
    "concordia",
    "codeceptjs",
    "webdriverio",
    "selenium",
    "test",
    "script",
    "e2e",
    "plugin",
    "plug-in"
  ],
  "main": "dist",
  "types": "dist",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "rimraf dist && tsc",
    "test": "jest",
    "postversion": "git push && git push --tags"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "concordiaPlugin": {
    "isFake": false,
    "targets": [
      "codeceptjs",
      "webdriverio"
    ],
    "file": "dist/CodeceptJS.js",
    "class": "CodeceptJS",
    "install": "npm install --color=always --save-dev mocha@v5.2.0 mocha-multi@v1.0.1 codeceptjs-cmdhelper codeceptjs-dbhelper database-js database-js-json && npm install --color=always -g codeceptjs@1.2.1 webdriverio@v4.14.0 selenium-standalone && selenium-standalone install",
    "uninstall": "npm uninstall --color=always --save-dev mocha mocha-multi codeceptjs-cmdhelper codeceptjs-dbhelper database-js database-js-json && npm uninstall --color=always -g codeceptjs webdriverio selenium-standalone",
    "serve": "selenium-standalone start"
  },
  "dependencies": {
    "concordialang-plugin": "^0.3.5",
    "concordialang-types": "^0.1.4",
    "dedent-js": "^1.0.1",
    "figures": "^2.0.0",
    "log-symbols": "^2.2.0",
    "mustache": "^2.3.0",
    "node-fs-extra": "^0.8.2"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "jest": "^24.5.0",
    "memfs": "^2.9.0",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.0",
    "typescript": "^3.3.4000"
  }
}
